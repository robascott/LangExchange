<h1>Users#index</h1>
<p>Find me in app/views/users/index.html.erb</p>

<ul>

<% @users.each do |user| %>
	<li>

		<a href="/users/<%= user['id'] %>"><%= user['username'] %></a>
		<p><%= user['email'] %></p>
		<p><%= user['language_id'] %></p>

		<ul>
			<% user.languages.each do |language| %>
				<li>
					<p><%= language.name %></p>
				</li>
			<% end %>
		</ul>
		

	</li>

<% end %>


</ul>
