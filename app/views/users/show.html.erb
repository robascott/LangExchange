<h1>Users#show</h1>


<a href="/users/<%= @user['id'] %>/notes/new">Add new note</a>


<ul>

<% @notes.each do |note| %>

	<li>
		<a href="/users/<%= @user['id'] %>/notes/<%= note['id'] %>"><%= note['title'] %></a>
		<%= link_to 'Delete', user_note_path(@user, note), :confirm => 'Are you sure?', :method => :delete %>
	</li>


<% end %>


</ul>
